            <TextView
                android:id="@+id/txt13"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="টিএসপি"
                android:layout_margin="2dp"
                android:background="#FFFFFF"
                android:gravity="center"
